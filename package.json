{
  "name": "auctionator-evm",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "dotenv -c dev -- nest start",
    "start:dev": "dotenv -c dev -- nest start --watch",
    "start:docker:dev": "docker compose -p auctionator-evm-back-dev -f dev-utils/docker-compose.dev.yml up -V -d --build --force-recreate -t 1",
    "start:debug": "dotenv -c dev -- nest start --debug --watch",
    "start:prod": "dotenv -c prod -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "dev:db:up": "docker-compose -p auctionator-evm-back-dev -f dev-utils/docker-compose.dev.yml up auctionator-evm-db-dev -d --force-recreate --build",
    "dev:db:down": "docker-compose -p auctionator-evm-back-dev -f dev-utils/docker-compose.dev.yml down auctionator-evm-db-dev",
    "dev:db:restore": "docker exec -i auctionator-evm-db-dev sh /opt/backups/restore.sh",
    "test:db:up": "docker-compose -p auctionator-evm-back-test -f dev-utils/docker-compose.test-db.yml up -d",
    "test:db:down": "docker-compose -p auctionator-evm-back-test -f dev-utils/docker-compose.test-db.yml down",
    "test:e2e": "dotenv -c test -- jest --runInBand --config test/jest-e2e.json",
    "upgrade-deps": "ncu"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^10.0.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "content-disposition": "^0.5.4",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "heic-convert": "^2.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "minio": "^7.1.3",
    "ms": "^2.1.3",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@types/bcrypt": "^5.0.2",
    "@types/content-disposition": "^0.5.8",
    "@types/express": "^4.17.21",
    "@types/heic-convert": "^1.2.3",
    "@types/jest": "29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/passport-jwt": "^3.0.13",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "jest": "29.7.0",
    "npm-check-updates": "^16.14.11",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "type-fest": "^4.8.3",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
